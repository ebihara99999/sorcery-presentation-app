<h1>Editing User</h1>

<% if request.path.match(/general_users/) %>
    <%= render 'general_user_form' %>
    <%= link_to 'Show', @user %> |
    <%= link_to 'Back', general_user_url %>
<% else %>
    <%= render 'admin_form' %>
    <%= link_to 'Show', @user %> |
    <%= link_to 'Back', admin_url %>
<% end %>

